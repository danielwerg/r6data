{
  "name": "r6data",
  "version": "0.0.0-alpha.1",
  "author": "Daniel Werg",
  "description": "Data for r6api.js",
  "license": "MIT",
  "repository": "danielwerg/r6data",
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist && npm run lint-tsc",
    "build": "vite build",
    "build:prepare": "path-exists ./dist || npm run build",
    "operatorNotes": "ts-node scripts/operatorNotes.ts",
    "svg": "ts-node scripts/svg.ts",
    "svg2png": "ts-node scripts/svg2png.ts",
    "preversion": "npm run build",
    "postversion": "git push --follow-tags",
    "lint-tsc": "tsc --noEmit",
    "lint": "eslint src scripts",
    "lint:fix": "eslint src scripts --fix",
    "spell": "cspell --show-context \"**/*.md\"",
    "prepare": "npm run build:prepare"
  },
  "prettier": "@danielwerg/prettier-config",
  "devDependencies": {
    "@danielwerg/eslint-config": "^1.0.0",
    "@danielwerg/prettier-config": "^1.0.0",
    "@danielwerg/tsconfig": "^0.1.0",
    "@prettier/plugin-xml": "^2.2.0",
    "@types/node": "^20.4.8",
    "@types/sharp": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "cheerio": "^1.0.0-rc.12",
    "cspell": "^6.31.2",
    "eslint": "^8.46.0",
    "eslint-plugin-import": "^2.28.0",
    "path-exists-cli": "^2.0.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "sharp": "^0.32.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.8",
    "vite-plugin-dts": "^3.5.1",
    "vite-plugin-svgo": "^1.3.0"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "assets",
    "svg",
    "icons",
    "ubisoft",
    "rainbow",
    "six",
    "siege",
    "rainbowsixsiege",
    "rainbow6",
    "rainbow6siege",
    "r6s"
  ]
}
